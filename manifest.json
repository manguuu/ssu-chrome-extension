{
   "name": "ssu-extension",
   "version": "1.0",
   "manifest_version": 2,
   "description": "숭실대학교 필수 사이트 바로가기",
  "browser_action": {
    "default_popup": "static/html/popup.html"
  },
  "permissions": [
    "tabs"  
  ],
  "web_accessible_resources": [
    "images/*"
  ],
  "icons": { "16": "img.png",
           "48": "img.png",
          "128": "img.png" }
}